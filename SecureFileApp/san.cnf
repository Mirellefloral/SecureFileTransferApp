[ req ]
default_bits        = 2048
prompt              = no
default_md          = sha256
req_extensions      = req_ext
distinguished_name  = dn

[ dn ]
C = US
ST = YourState
L = YourCity
O = YourOrganization
OU = YourOrganizationalUnit
CN = 192.168.1.75

[ req_ext ]
subjectAltName = @alt_names

[ alt_names ]
IP.1 = 172.20.10.3
